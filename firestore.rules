rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // Allow read/write access to the cds collection for all users
    // This is appropriate for a personal app without authentication
    match /cds/{document=**} {
      allow read, write: if true;
    }
    
    // Allow read/write access to the dvds collection for all users
    match /dvds/{document=**} {
      allow read, write: if true;
    }
  }
}
